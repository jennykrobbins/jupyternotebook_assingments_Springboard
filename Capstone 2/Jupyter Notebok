   "cell_type": "code",
   "execution_count": 12,
   "id": "853eaa7c",
   "metadata": {},
   "outputs": [],
   "source": [
    "data['Total Words'] = data['Text'].apply(lambda x: len(x.split()))\n",
    "\n",
    "def count_total_words(text):\n",
    "    char = 0\n",
    "    for word in text.split():\n",
    "        char += len(word)\n",
    "    return char\n",
    "\n",
    "data['Total Chars'] = data[\"Text\"].apply(count_total_words)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "id": "d80ade44",
   "metadata": {},
   "outputs": [],
   "source": [
    "def convert_lowercase(text):\n",
    "    text = text.lower()\n",
    "    return text\n",
    "\n",
    "data['Text'] = data['Text'].apply(convert_lowercase)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "94c1e29a",
   "metadata": {},
   "outputs": [],
   "source": [
    "exclude = string.punctuation\n",
    "\n",
    "def remove_punc(text):\n",
    "    return text.translate(str.maketrans('', '', exclude))\n",
    "\n",
    "data['Text'] = data['Text'].apply(remove_punc)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "id": "369ebc71",
   "metadata": {},
   "outputs": [],
   "source": [
    "def remove_stopwords(text):\n",
    "    new_list = []\n",
    "    words = word_tokenize(text)\n",
    "    stopwrds = stopwords.words('english')\n",
    "    for word in words:\n",
    "        if word not in stopwrds:\n",
    "            new_list.append(word)\n",
    "    return ' '.join(new_list)\n",
    "\n",
    "data['Text'] = data['Text'].apply(remove_stopwords)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "78a9a9da",
   "metadata": {},
   "outputs": [],
   "source": [
    "def perform_stemming(text):\n",
    "    stemmer = PorterStemmer()\n",
    "    new_list = []\n",
    "    words = word_tokenize(text)\n",
    "    for word in words:\n",
    "        new_list.append(stemmer.stem(word))\n",
    "\n",
    "    return \" \".join(new_list)\n",
    "\n",
    "data['Text'] = data['Text'].apply(perform_stemming)"
   ]
  },
